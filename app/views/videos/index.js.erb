$('#videos').append('<%= j render "video" %>');
<% if @videos.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @videos, hidden: true %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
  $('#request_video_div').fadeIn('slow');
<% end %>
$('.bricks-wrapper').masonry( 'reloadItems' );
$('.bricks-wrapper').masonry( 'layout' );